$('body').click(function(e){
    var Elem = e.target;
    if (Elem.id == 'commentaire'){
        bootbox.prompt("{{ 'suivi.commentaire'|trans({}, 'project') }} :", function(result) {
        if (result !== null) {
            $.ajax({
                url: "{{ path('project_state') }}",
                type: "PUT",
                data: "state=" + result +"&etude=" + $(Elem).data("title") + "&id=" + $(Elem).data("content"),
                success: function(){}
                });
            }
        })
    }
});
